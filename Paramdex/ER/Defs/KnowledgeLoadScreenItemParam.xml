<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>KNOWLEDGE_LOADSCREEN_ITEM_PARAM_ST</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="u8 disableParam_NT:1">
      <DisplayName>Disable Param - Network Test</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Parameters marked with ○ are excluded in the NT version package.</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="dummy8 disableParamReserve1:7">
      <DisplayName>Reserve for package output 1</DisplayName>
      <Description>Reserve for package output 1</Description>
      <DisplayFormat>%d</DisplayFormat>
      <SortID>301</SortID>
    </Field>
    <Field Def="dummy8 disableParamReserve2[3]">
      <DisplayName>Reserve for package output 2</DisplayName>
      <Description>Reserve for package output 2</Description>
      <DisplayFormat>%d</DisplayFormat>
      <SortID>302</SortID>
    </Field>
    <Field Def="u32 unlockFlagId">
      <DisplayName>Unlock Flag ID</DisplayName>
      <Description>Lifting flag (default: 0). If this event flag is set, the ban will be lifted (displayed on the loading screen). If it is 0, the ban is always lifted. The invalid flag has priority</Description>
      <Maximum>2100000000</Maximum>
      <SortID>200</SortID>
    </Field>
    <Field Def="u32 invalidFlagId">
      <DisplayName>Invalid Flag ID</DisplayName>
      <Description>Invalid flag (default: 0). If this event flag is set, it will be disabled (it will not be displayed on the loading screen). If it is 0, this flag is not always set.</Description>
      <Maximum>2100000000</Maximum>
      <SortID>300</SortID>
    </Field>
    <Field Def="s32 msgId">
      <DisplayName>Message ID</DisplayName>
      <Description>Text ID (Loading Text.xlsx). Used for both loading titles and loading text</Description>
      <Minimum>-1</Minimum>
      <Maximum>2100000000</Maximum>
      <SortID>100</SortID>
    </Field>
  </Fields>
</PARAMDEF>