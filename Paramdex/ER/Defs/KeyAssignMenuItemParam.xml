<?xml version="1.0" encoding="utf-8"?>
<PARAMDEF XmlVersion="2">
  <ParamType>CS_KEY_ASSIGN_MENUITEM_PARAM</ParamType>
  <DataVersion>1</DataVersion>
  <BigEndian>False</BigEndian>
  <Unicode>True</Unicode>
  <FormatVersion>203</FormatVersion>
  <Fields>
    <Field Def="s32 textID">
      <DisplayName>Text ID</DisplayName>
      <Description>Key specified ⇒ Item name, 1-line help ID. No key specified ⇒ Category name ID. No text specified ⇒ Not displayed in pad / key settings (operation list display only)</Description>
      <Minimum>0</Minimum>
      <Maximum>2100000000</Maximum>
      <SortID>200</SortID>
      <UnkC8>割り当て項目</UnkC8>
    </Field>
    <Field Def="s32 key = -1">
      <DisplayName>Key</DisplayName>
      <Enum>CS_USER_INPUT_KEY</Enum>
      <Description>User input key to be assigned. If not specified, it will be treated as a category display item.</Description>
      <Minimum>-1</Minimum>
      <Maximum>9999</Maximum>
      <SortID>201</SortID>
      <UnkC8>割り当て項目</UnkC8>
    </Field>
    <Field Def="u8 enableUnassign = 1">
      <DisplayName>Allow Unassignment</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Is it possible to unassign (default: possible)</Description>
      <Maximum>1</Maximum>
      <SortID>300</SortID>
      <UnkC8>割り当て制限</UnkC8>
    </Field>
    <Field Def="u8 enablePadConfig = 1">
      <DisplayName>Allow Pad Config</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Is it possible to change the pad operation assignment (default: possible)?</Description>
      <Maximum>1</Maximum>
      <SortID>301</SortID>
      <UnkC8>割り当て制限</UnkC8>
    </Field>
    <Field Def="u8 enableMouseConfig = 1">
      <DisplayName>Allow Mouse Config</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Is it possible to change the mouse operation assignment (default: possible)?</Description>
      <Maximum>1</Maximum>
      <SortID>302</SortID>
      <UnkC8>割り当て制限</UnkC8>
    </Field>
    <Field Def="u8 group">
      <DisplayName>Group</DisplayName>
      <Enum>CS_KEY_ASSIGN_GROUP</Enum>
      <Description>Group for determining duplicate assignments. Cannot be duplicated in the same group</Description>
      <Maximum>1</Maximum>
      <SortID>303</SortID>
      <UnkC8>割り当て制限</UnkC8>
    </Field>
    <Field Def="s32 mappingTextID">
      <DisplayName>Mapping Text ID</DisplayName>
      <Description>Item name to be displayed in the operation list. 0 :: Do not display in the list</Description>
      <Minimum>0</Minimum>
      <Maximum>2100000000</Maximum>
      <SortID>400</SortID>
      <UnkC8>操作一覧表示</UnkC8>
    </Field>
    <Field Def="u8 viewPad = 1">
      <DisplayName>View Pad</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Whether to display with key config (pad) (default: display)</Description>
      <Maximum>1</Maximum>
      <SortID>100</SortID>
      <UnkC8>表示条件</UnkC8>
    </Field>
    <Field Def="u8 viewKeyboardMouse = 1">
      <DisplayName>View Keyboard/Mouse</DisplayName>
      <Enum>BOOL_CIRCLECROSS_TYPE</Enum>
      <Description>Whether to display with key config (mouse / keyboard) (default: display)</Description>
      <Maximum>1</Maximum>
      <SortID>101</SortID>
      <UnkC8>表示条件</UnkC8>
    </Field>
    <Field Def="dummy8 padding[6]">
      <DisplayName>padding</DisplayName>
      <Description>Is it possible to change the pad operation assignment (default: possible)?</Description>
      <SortID>99999</SortID>
    </Field>
  </Fields>
</PARAMDEF>